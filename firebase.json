{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [{
      "source": "/resources/modules/+([0-9]).+([0-9]).+([0-9])/:path*",
      "destination": "../:path"
    }],
    "rewrites": [{
      "source": "!.@(js|css)",
      "destination": "/index.html"
    }]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
